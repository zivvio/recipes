{
    "name": "ziphp/recipes",
    "description": "ziphp recipes",
    "type": "library",
    "keywords": [
        "ziphp",
        "recipes"
    ],
    "homepage": "https://github.com/ziphp/recipes",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/ziphp/recipes",
        "issues": "https://github.com/ziphp/recipes/issues"
    },
    "authors": [
        {
            "name": "charescape",
            "homepage": "https://github.com/charescape"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "charescape/php-functions": ">=1.1.0",
        "lcobucci/clock": "^3.0",
        "lcobucci/jwt": "^5.3",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/string": "^6.4|^7.0",
        "symfony/var-dumper": "^6.4|^7.0",
        "ziphp/ziphp": ">=5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "ziphp/zipper": true
        }
    },
    "autoload": {
        "psr-4": {
            "Ziphp\\Recipes\\": "src/"
        }
    },
    "scripts": {
        "iii": [
            "rm -f composer.lock",
            "@composer self-update -vvv",
            "@composer install -vvv"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "6.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
